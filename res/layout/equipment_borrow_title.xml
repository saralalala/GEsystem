    <TextView
        android:id="@+id/equipment4"
        style="@style/titlestyle"
        android:text="经手人">
    </TextView>
    
    <TextView
        android:id="@+id/equipment5"
        style="@style/titlestyle"
        android:text="借用日期">  
    </TextView>
    
    <TextView
        android:id="@+id/equipment6"
        style="@style/titlestyle"
        android:text="借用时间">  
    </TextView>
    
    <TextView
        android:id="@+id/equipment7"
        style="@style/titlestyle"
        android:text="归还日期">  
    </TextView>
    
    <TextView
        android:id="@+id/equipment8"
